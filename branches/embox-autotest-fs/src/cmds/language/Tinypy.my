package embox.cmd

@Cmd(name = "tinypy",
	help = "Python interpreter",
	man = '''
		NAME
			tinypy - Python interpreter
		SYNOPSIS
			tinypy [script]
		AUTHORS
			Nikolay Korotkiy
	''')
module tinypy {
	@IncludePath("$(THIRDPARTY_DIR)/tinypy")
	source "tinypy.c"

	depends third_party.tinypy.tinypy_core
	depends embox.framework.LibFramework
}
