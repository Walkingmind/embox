#!/usr/bin/runtest

source [file join [file dirname [info script]] exec_through_telnet.exp]

proc ping_test {} {
	global host_ip

	send "ping -c 1 $host_ip\r\n"
	expect "ping"
	expect {
	        timeout  { puts "ping.exp: timeout\n"; return -1 }
	        "+0 errors" { }
	}
	return 0
}

exec_cmd "$embox_ip" ping_test
