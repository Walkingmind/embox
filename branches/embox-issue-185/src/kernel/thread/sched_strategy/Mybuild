package embox.kernel.thread.sched_policy

abstract module api { }

module priority_based extends api {
	source "priority_based.c", "priority_based.h"
	
	option number tick_interval = 100
}

module trivial extends api {
	source "trivial.c", "trivial.h"

}

module fixedtime extends api {
	source "fixedtime_sched.c", "fixedtime_sched.h"

	option number msec_priority_multiplier = 1
}
