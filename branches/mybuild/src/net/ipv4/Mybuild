package embox.net

module af_inet {
	source "af_inet.c", "protocol.c", "route.c", "ipconfig.c"
	depends embox.net.socket
	depends embox.net.raw_sock
}

module arp {
	source "arp.c"
	depends embox.net.neighbour
}

module ipv4 {
	source "ip_input.c", "ip_options.c", "ip_output.c", "ip_fragment.c"
}

module icmp {
	source "icmp.c"
	depends af_inet
}

module udp {
	source "udp.c"
	depends embox.net.udp_sock
}

module tcp {
	source "tcp.c"
	depends embox.net.tcp_sock
}

module raw_sock {
	source "raw_sock.c"
	depends embox.net.socket
}

module udp_sock {
	source "udp_sock.c"
	depends embox.net.socket
}

module tcp_sock {
	source "tcp_sock.c"
	depends embox.net.socket
}
