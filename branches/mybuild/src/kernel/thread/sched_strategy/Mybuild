package embox.kernel.thread.sched_policy

abstract module api { }

module sched_trivial extends api {
	@IncludePath("$(SRC_DIR)/compat/posix/include")
	source "sched_trivial.c", "sched_trivial.h"

}

module priority_based extends api {
	@IncludePath("$(SRC_DIR)/compat/posix/include")
	source "priority_based.c", "priority_based.h"

}

module fixedtime_sched extends api {
	@IncludePath("$(SRC_DIR)/compat/posix/include")
	source "fixedtime_sched.c", "fixedtime_sched.h"

}
