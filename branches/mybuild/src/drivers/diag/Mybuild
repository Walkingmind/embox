package embox.driver.diag

abstract module diag_api {
}

module no_diag extends diag_api {
	source "no_diag.c"
}

module apbuart extends diag_api {
	source "apbuart.c"
	depends embox.driver.ambapp
}

module serial_port extends diag_api {
	source "serial_port.c"
}

module at91_uart extends diag_api {
	source "at91_uart.c"
}

module xuartlite extends diag_api {
	source "xuartlite.c"
}

module vga extends diag_api {
	source "vga.c"
	depends embox.driver.video.vga, embox.driver.keyboard
	make flags "-I$(SRC_DIR)/compat/posix/include"
}

