package embox.cmd

@Cmd(name = "sample",
	man = '''
		NAME
			sample - tool for sampling profiling
		SYNOPSIS
			sample [options]
		DESCRIPTION
			Tool for profiling.
		OPTIONS
			-l [num] - show less functions (constraints %)
			-h - print usage
			-s - start profiler (restart if already running)
			-t - stop profiler
			-i - set custom timer interval
		AUTHORS
			Denis Deryugin
	''')

module sample {
	source "sample.c"

	depends embox.lib.LibCStdio
	depends embox.framework.LibFramework
}
