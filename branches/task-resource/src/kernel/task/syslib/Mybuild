package embox.kernel.task.syslib

@DefaultImpl(no_kfork)
abstract module kfork_api {
}

module kfork extends kfork_api {
	source "kfork.c"
}

module no_kfork extends kfork_api {
	source "no_kfork.c"
}

module waitpid {
	source "waitpid.c"

	depends embox.kernel.sched.sched
	depends embox.kernel.sched.wait_queue
	depends embox.kernel.task.resource.waitpid
}
