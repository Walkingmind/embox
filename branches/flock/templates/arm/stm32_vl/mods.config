
package genconfig

configuration conf {
	@Runlevel(0) include embox.arch.arm.lds
	@Runlevel(0) include embox.arch.arm.kernel.arch
	@Runlevel(0) include embox.arch.arm.cortexm3.kernel.locore
	@Runlevel(0) include embox.arch.system(core_freq=8000000)
	
	@Runlevel(0) include embox.kernel.stack(stack_size=2048,alignment=4)
	
	include embox.kernel.thread.sched_policy.nosched
	include embox.mem.heap_bm(heap_size=0x100)
	include embox.mem.bitmask(page_size=32)
	include embox.mem.static_heap(heap_size=0x100)
	
	include embox.kernel.task.idx_table(task_res_quantity=8)

	include embox.kernel.task.api(max_resource_size=256)

	@Runlevel(1) include embox.driver.interrupt.stm32_nvic
	@Runlevel(1) include embox.driver.clock.stm32_systick
	@Runlevel(1) include embox.driver.diag.stm32_usart
	@Runlevel(1) include embox.kernel.timer.sys_timer
	@Runlevel(1) include embox.kernel.timer.strategy.list_timer
	@Runlevel(1) include embox.kernel.time.kernel_time

	@Runlevel(3) include embox.init.start_script
	include embox.cmd.sh.tish(amp_support=0)
	include embox.cmd.version
	include embox.cmd.help

	include embox.kernel.critical
	include embox.kernel.irq
	include embox.mem.pool_adapter
	include embox.prom.prom_printf
	include embox.util.LibUtil
	include embox.framework.LibFramework
	include embox.arch.arm.libarch
	include embox.lib.LibCStdio(support_floating=0)

	include embox.util.num_alloc_array

	@Runlevel(2) include embox.init.make_term

	/*@Runlevel(2) include stm32vl.test.blinking_led*/
}
