package third_party.qt

@Build(script="$(MAKE) -C $(THIRDPARTY_DIR)/qt MAKEFLAGS= EMBOX_ARCH='$(ARCH)' EMBOX_CROSS_COMPILE='$(CROSS_COMPILE)' EMBOX_MAKEFLAGS='$(MAKEFLAGS)' ROOT_DIR=$(abspath $(ROOT_DIR)) EMBOX_CFLAGS='$(CFLAGS)' EMBOX_CXXFLAGS='$(CXXFLAGS)' EMBOX_CPPFLAGS='$(EMBOX_EXPORT_CPPFLAGS)'")
static module core {
	depends embox.lib.LibC
	depends embox.lib.cxx.lib
	depends embox.compat.posix.fs.dirent
	depends embox.fs.rootfs
	depends embox.compat.posix.LibPosix

	source
		"../../../../qt/install/lib/libQtCore.a",
		"../../../../qt/install/lib/libQtGui.a",
		"../../../../qt/install/lib/libQtMultimedia.a",
		"../../../../qt/install/lib/libQtNetwork.a",
		"../../../../qt/install/lib/libQtSql.a",
		"../../../../qt/install/lib/libQtSvg.a",
		"../../../../qt/install/lib/libQtTest.a",
		"../../../../qt/install/lib/libQtXml.a"
}
