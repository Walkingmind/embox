package third_party.qt.plugin.platform

@Build(script="TARGET=$(EXTERNAL_BUILD_DIR)/third_party/qt/core/source/src/plugins/platforms/vnc; $(EXTERNAL_MAKE_PRO)")
@BuildDepends(third_party.qt.core)
module vnc {
	@AddPrefix("^BUILD/extbld/third_party/qt/core/build/plugins/platforms")
	source "libqvncgraphicssystem.a"

/*
	@IncludePath("$(abspath $(EXTERNAL_BUILD_DIR))/third_party/qt/core/build/include")
	@IncludePath("$(abspath $(EXTERNAL_BUILD_DIR))/third_party/qt/core/build/include/QtCore")
	@IncludePathBefore("$(abspath $(EXTERNAL_BUILD_DIR))/third_party/STLport/core/include/stlport")
*/
	source "vnc.cpp"

	@NoRuntime depends third_party.qt.core
	@NoRuntime depends third_party.qt.plugin.platform.minimal
}
