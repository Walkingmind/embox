#include <stdio.h>

int main()
{
	FILE * f;
	f = fopen("/tmp/TEST.TXT", "w");
	if (f == NULL) {
		printf("File creation failed\n");
		return 1;
	}
	fprintf(f, "TEST");
	printf("\n\nHello world!\n\n");
	fclose(f);
	return 0;
}
