
package('embox.fs.driver')

module(
	'nfs',

  	options = [
		Integer('inode_quantity', default=264),
 		Integer('nfs_descriptor_quantity', default=4),
	],

 	sources = [
		'nfs.c',
 		'xdr_nfs.c',
	],

 	depends = [
		'embox.net.rpc',
 		'embox.fs.node',
 		'embox.fs.driver.repo',
 		'embox.fs.driver.devfs',
 		'embox.mem.page_api',
 		'embox.mem.pool',
	],

) 
