
package('embox.fs.driver')

module(
	'cdfs',

  	options = [
		Integer('inode_quantity', default=2048),
 		Integer('cdfs_descriptor_quantity', default=2),
	],

 	sources = [
		'cdfs.c',
	],

 	depends = [
		'embox.fs.node',
 		'embox.fs.driver.repo',
 		'embox.driver.block',
 		'embox.mem.page_api',
 		'embox.mem.pool',
	],

) 
