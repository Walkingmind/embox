package third_party.busybox

module core {
	depends embox.lib.LibC
	depends embox.lib.cxx.lib
	depends embox.arch.x86.LibM // FIXME:

	source "empty.c" // FIXME: workaround

	@Rule(script="$(MAKE) -C $(THIRDPARTY_DIR)/busybox MAKEFLAGS= EMBOX_MAKEFLAGS='$(MAKEFLAGS)' ROOT_DIR=$(abspath $(ROOT_DIR)) EMBOX_CFLAGS='$(CFLAGS)' EMBOX_CXXFLAGS='$(CXXFLAGS)' EMBOX_CPPFLAGS='$(EMBOX_EXPORT_CPPFLAGS)'")
	source
		"../../../../busybox/bin/busybox.embox.o"
}
