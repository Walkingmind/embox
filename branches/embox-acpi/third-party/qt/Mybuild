package third_party.qt

module core {
	depends embox.lib.LibC
	depends embox.lib.cxx.lib
	depends embox.compat.posix.fs.dirent
	depends embox.fs.rootfs
	depends embox.compat.posix.LibPosix
	source "empty.c"
}

module animatedtiles extends embox.lib.cxx.app {
	source "empty.c"
	@Rule(script="$(MAKE) -C $(THIRDPARTY_DIR)/qt MAKEFLAGS= EMBOX_ARCH='$(ARCH)' EMBOX_CROSS_COMPILE='$(CROSS_COMPILE)' EMBOX_MAKEFLAGS='$(MAKEFLAGS)' ROOT_DIR=$(abspath $(ROOT_DIR)) EMBOX_CFLAGS='$(CFLAGS)' EMBOX_CXXFLAGS='$(CXXFLAGS)' EMBOX_CPPFLAGS='$(EMBOX_EXPORT_CPPFLAGS)'")
	source "../../../../qt/apps_embox/animatedtiles/animatedtiles.embox.o"
	depends core
}

