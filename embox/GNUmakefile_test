ifndef __mk_ready
MAKEFLAGS += --no-builtin-rules
MAKEFLAGS += --no-builtin-variables
MAKEFLAGS += --no-print-directory
MAKEFLAGS += --warn-undefined-variables
.DEFAULT_GOAL := all
% :
	@$(MAKE) __mk_ready=1 $@
else

include mk/gold/engine.mk
include mk/mybuild/myfile.mk

MY_FILES := ./src/kernel/Irq.my


MKFILES_CONVERTED := \
	build/Irq.my.mk

.PHONY : all $(MKFILES_CONVERTED)
all : $(MKFILES_CONVERTED) mk_objects_dump mk_object_to_mk

$(MKFILES_CONVERTED) : mk/mybuild/read.mk mk/mybuild/myfile-rules.mk

include mk/util/escape.mk

build/Irq.my.mk : ./src/kernel/Irq.my
	@echo '$< -> $@'
	@mkdir -p $(@D);
	@printf '\n%b\n\n' '$(call escape_printf,$(call model_to_resource,$(call gold_parse,myfile,$<)))' > $@
	@cat $@

endif

