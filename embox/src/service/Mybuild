package embox.service

module SrvLib {
	source "srv_lib.c"
	
	depends embox.lib.service.service
}

module ImgService {
	//source "img_service.c"
	depends embox.service.SrvLib
	depends embox.framework.service
}

module TestService {
	@IncludePath("$(THIRDPARTY_DIR)/lib/expat")
	source "test_service.c" 
	
	@InitFS
	source "test.html"
	
	depends SrvLib
	depends embox.third_party.expat.expat_lib
	depends embox.framework.service
}

module NewService {
	source "new_service.c"
	option string template_file="img_service.in"
	
	@InitFS
	source "img_service.in"
	
	depends SrvLib
	depends embox.framework.service
}

@AutoCmd
@Cmd(name="cgi_cmd_wrapper", help="", man="")
module cgi_cmd_wrapper {
	source "cgi_cmd_wrapper.c"
}
