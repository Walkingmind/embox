package embox.kernel

abstract module tasks_api { }

module task_common {
	source "task_common.c", "file.c"
	depends embox.fs.core, embox.fs.rootfs
}

module tasks extends tasks_api {
	source "task.c"
	depends embox.driver.virtual.diag_device, task_common
	depends embox.kernel.thread.sched
}

module no_tasks extends tasks_api {
	source "no_task.c"
	depends embox.driver.virtual.diag_device, task_common
}
