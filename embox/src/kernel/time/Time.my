package embox.kernel.time

module clock_source {
	@NumConstraint(ge=1)
	option number clocks_quantity = 4

	source "clock_source.c"

	depends embox.arch.clock
	depends kernel_time
	depends timer
}

module kernel_time {
	source "ktime.c"
}

module jiffies {
	source "jiffies.c"
}

module timer {
	source "timer.c"
	depends jiffies
}
