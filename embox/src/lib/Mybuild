	source "stdio/fputc.c"
	source "stdio/gets.c"
	source "stdio/sprintf.c"
	source "stdio/printf.c"
	source "stdio/printf_impl.c"
	source "stdio/printk.c"
	source "stdio/puts.c"
	source "stdio/scanf.c"
	source "stdio/perror.c"
