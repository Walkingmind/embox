package embox.lib

module LibCXX {
	/* Generic files */
	source "new.cpp"
	source "exception.cpp"

	/* Private C++ files */
	source "unwind.c"
	source "eh_routines.cpp"
        source "purevirt_routines.cpp"
	
	/*linker sections */
	source "cxx.lds.S"
	source "cxx_init.c"

	depends embox.lib.LibC
}

abstract module CXXApp {
	/* The implementation of this module is supposed to
		implement the "int main(int argc, char** argv)" */
}
