package embox.lib.debug

static module Symbol {
	source "symbol.c"
}

@Mandatory
@DefaultImpl(Nobacktrace)
abstract module backtrace_api {	
}

static module Backtrace extends backtrace_api {
	source "backtrace.c", "backtrace.h"
	depends embox.arch.stackframe
	depends embox.lib.debug.Symbol
}

static module Nobacktrace extends backtrace_api {
	source "nobacktrace.h"
}
