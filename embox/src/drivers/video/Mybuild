package embox.driver.video

module fb {
	source "fb.c"
	option number fb_amount=2
	depends embox.mem.pool
	depends embox.lib.LibC
	depends embox.fs.rootfs
	depends modedb
}

module modedb {
	source "modedb.c"
}

module display {
	source "display.c"
	depends embox.util.List
	depends embox.lib.LibC
}

module bochs {
	option number default_mode=0x114
	source "bochs.c"

	depends fb
	depends vesa
	depends embox.driver.pci
	depends embox.lib.LibC
}

module cirrus_logic {
	source "cirrus_logic.c"
}
