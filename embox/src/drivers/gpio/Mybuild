package embox.driver.gpio

abstract module pins {
	
}

module at91_pins extends pins {
	source "at91_pins.c", "pins.c"

	depends embox.kernel.irq
}

module rdc_gpio {
	source "rdc_gpio.c"
}

module stm32 {
	@IncludePath("$(PLATFORM_DIR)/stm32vl/include")
	source "stm32_gpio.c"

	depends embox.lib.LibC
}
