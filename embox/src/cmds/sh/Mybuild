package embox.cmd.sh

abstract module shell_api { }

module tish extends shell_api {

	@IncludePath("$(THIRDPARTY_DIR)/include")
	source "tish.c"

	depends third_party.lib.linenoise 
}

module shell_start {
    @IncludePath("$(CONF_DIR)")
	source "shell_start.c"
	depends embox.kernel.thread.core
	depends shell_api
}
