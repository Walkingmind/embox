package embox.cmd

import embox.compat.posix.LibPosix

@Cmd(name = "mount",
	help = "Display last debug messages..",
	man = '''
	NAME
		mount - mount device into directory
	SYNOPSIS
		mount DEV DIR
	DESCRIPTION
		Note that one does not really mount a device, one mounts
        a filesystem found on the device.
	''')
module mount {
	source "mount.c"
	depends embox.fs.ramfs, embox.fs.core
}
