package embox.cmd

import embox.arch.interrupt

@Cmd(name = "goto",
	help = "Transfer of control flow",
	man = '''
		NAME
			goto - one-way control transfer
		SYNOPSIS
			goto [-h] [-a address]
		DESCRIPTION
			Executes the code at the specified address.
		OPTIONS
			-h
				Show usage
			-a address
				Entry point
		EXAMPLES
			goto -a 0x40000000
		SEE ALSO
			load
		AUTHORS
			Sergey Kuzmin
	''')
module goto {
	source "goto.c"
	depends interrupt
}
