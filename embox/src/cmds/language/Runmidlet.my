package embox.cmd

@AutoCmd
@Cmd(name = "runMidlet",
	help = "runMidlet application on top of jvm",
	man = '''
		NAME
			runMidlet - runMidlet application on top of jvm
		SYNOPSIS
			runMidlet [arguments]
		AUTHORS
			Felix Sulima
	''')
module runMidlet {
	@IncludePath("$(THIRDPARTY_DIR)/phoneme")
	source "runmidlet.c"

	depends third_party.phoneme.midp
	depends embox.framework.LibFramework
}
