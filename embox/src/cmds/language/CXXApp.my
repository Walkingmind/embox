package embox.cmd

@AutoCmd
@Cmd(name = "cxxapp",
	help = "Run built-in C++ application",
	man = '''
		NAME
			cxxapp - Run built-in C++ application
		SYNOPSIS
			cxxapp [application_arguments]
		AUTHORS
			Felix Sulima
	''')
module cxxapp {
	@IncludePath("$(SRC_DIR)/lib/c++")
	source "cxxapp.c"

	depends embox.lib.cxx.AppStartupShutdown
	depends embox.framework.LibFramework
}
