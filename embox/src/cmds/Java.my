package embox.cmd

@Cmd(name = "java",
	help = "CLDC java virtual machine",
	man = '''
		NAME
			java - CLDC java virtual machine
		SYNOPSIS
			java [arguments]
		AUTHORS
			Felix Sulima
	''')
module java {
	@IncludePath("$(THIRDPARTY_DIR)/phoneme")
	source "java.c"

	depends third_party.phoneme.cldc_standalone
	depends embox.framework.LibFramework
}
