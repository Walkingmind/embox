package embox.cmd.net

@Cmd(name = "getmail",
	help = "Get email",
	man = '''
		NAME
			getmail - get email from mail server
		SYNOPSIS
			getmail <user> <password> <server>
		DESCRIPTION
			get mail from mail server
		EXAMPLES
			getmail nick 123456 pop.mail.ru
		SEE ALSO
			sendmail
		AUTHORS
			Ilia Vaprol
	''')
module getmail {
	source "getmail.c"

	depends embox.net.pop3
	depends embox.lib.LibC
	depends embox.framework.LibFramework
}
