package embox.fs.driver

module repo {
	option number drivers_quantity = 8
    @IncludePath("$(SRC_DIR)/compat/posix/include")
	source "fs_drv.c"
}


module fat {
	source "fat.c"
	depends embox.fs.node, embox.fs.driver.repo
	depends embox.fs.driver.devfs
	depends embox.mem.page_api
}

module devfs {
	@IncludePath("$(SRC_DIR)/compat/posix/include")
	source "devfs.c"
	depends embox.fs.rootfs
}

module stdio {
	source "stdio.c"
	depends embox.kernel.task_api
}


