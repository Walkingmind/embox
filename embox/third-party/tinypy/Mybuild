package third_party

@AutoCmd
@Cmd(name = "tinypy",
	help = "Python interpreter",
	man = '''
		NAME
			tinypy - Python interpreter
		SYNOPSIS
			tinypy [script]
		AUTHORS
			Nikolay Korotkiy
	''')
module tinypy {
	@IncludePath("$(THIRDPARTY_DIR)/tinypy")
	source "tinypy.c"

	@IncludePath("$(SRC_DIR)/include/util")
	source "tp.c"

	@InitFS
	source "fib.py", "py2bc.py"

	depends embox.compat.libc.LibC
	depends embox.compat.posix.LibPosix
	depends embox.compat.libc.stdlib.system
	depends embox.framework.LibFramework
}
