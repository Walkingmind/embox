@Cmd(name = "dropbear",
	help = "Dropbear SSH server",
	man = '''
	''')
module dropbear {
	@IncludePath("$(THIRDPARTY_DIR)/dropbear-2012.55")
	@IncludePath("$(THIRDPARTY_DIR)/dropbear-2012.55/libtomcrypt/src/headers")
	@IncludePath("$(THIRDPARTY_DIR)/dropbear-2012.55/libtommath")
	@IncludePath("$(SRC_DIR)/compat/posix/include")
	@IncludePath("$(SRC_DIR)/compat/linux/include")
	source
    "dbutil.c",
    "buffer.c",
    "dss.c",
    "bignum.c",
    "signkey.c",
    "rsa.c",
    "random.c",
    "queue.c",
    "atomicio.c",
    "compat.c",
    "fake-rfc2553.c",
    "common-session.c",
    "packet.c",
    "common-algo.c",
    "common-kex.c",
    "common-channel.c",
    "common-chansession.c",
    "termcodes.c",
    "loginrec.c",
    "tcp-accept.c",
    "listener.c",
    "process-packet.c",
    "common-runopts.c",
    "circbuffer.c",
    "svr-kex.c",
    "svr-algo.c",
    "svr-auth.c",
    "sshpty.c",
    "svr-authpasswd.c",
    "svr-authpubkey.c",
    "svr-authpubkeyoptions.c",
    "svr-session.c",
    "svr-service.c",
    "svr-chansession.c",
    "svr-runopts.c",
    "svr-agentfwd.c",
    "svr-main.c",
    "svr-x11fwd.c",
    "svr-tcpfwd.c",
    "svr-authpam.c",
    "embox.c"
    
	depends libtommath
	depends libtomcrypt
	depends embox.compat.posix.util.All
	depends embox.kernel.time.timekeeper /* TODO */
}

module dropbearkey {
	@IncludePath("$(THIRDPARTY_DIR)/dropbear-2012.55")
	@IncludePath("$(THIRDPARTY_DIR)/dropbear-2012.55/libtomcrypt/src/headers")
	@IncludePath("$(THIRDPARTY_DIR)/dropbear-2012.55/libtommath")
	@IncludePath("$(SRC_DIR)/compat/posix/include")
	source
	//"dbutil.c",
	//"buffer.c",
	//"dss.c",
	//"bignum.c",
	//"signkey.c"
	//"rsa.c",
	//"random.c",
	//"queue.c",
	//"atomicio.c",
	//"compat.c",
	//"fake-rfc2553.c",
	"dropbearkey.c",
	"gendss.c",
	"genrsa.c"
	//"embox.c"
	@InitFS
	source "dropbear_rsa_host_key",
	"random"
	
		
	depends libtommath
	depends libtomcrypt
}
