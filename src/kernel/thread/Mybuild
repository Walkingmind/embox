package embox.kernel.thread;

//import foo.bar.*;

module core {
	source "core.c";

	depends sched, sync, embox.kernel.tasks_api;

	depends embox.hal.context;
}

module sched {
	source "sched.c";

	depends embox.kernel.timer.timer_api; // for timeslices support
