
package('embox.lib.debug')

module (
    'Symbol',
    sources = ["symbol.c"]
)

interface (
    'backtrace_api',
    default = 'Nobacktrace',
    mandatory = True,
)

module (
    'Backtrace',

    implements = ['backtrace_api'],
    sources = ["backtrace.c", "backtrace.h"],
    depends = [
        'embox.arch.stackframe',
	'embox.lib.debug.Symbol'
    ]
)

module ( 
    'Nobacktrace',
    implements = ['backtrace_api'],

    sources = ["nobacktrace.h"],
)
