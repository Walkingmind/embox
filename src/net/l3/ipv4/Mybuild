package embox.net

module ipv4 {
	source "ip_fragment.c"

	option number ip_fragmented_support = 1

	depends skbuff
	depends embox.mem.objalloc
	depends embox.kernel.timer.sys_timer

	source "ip_input.c"
	depends skbuff
	depends raw_sock
	/* depends from ip_fragment.c */
	depends netfilter

	source "ip_options.c"
	depends embox.net.icmpv4

	source "ip_output.c"
	
	source "ip_header.c"
	
	depends net_tx
	depends skbuff
	depends route
	depends packet
	depends embox.net.lib.ipv4
}
