package embox.cmd

@Cmd(name = "ide",
	help = "Check IDE drive",
	man = '''
		NAME
			ide - check IDE drive
		SYNOPSIS
			ide 
		DESCRIPTION
			ide scan ports and find ide drives 
		OPTIONS
			none
		AUTHOR
			Andrey Gazukin
	''')
module ide {
	source "ide.c"

	depends embox.driver.ide
	depends embox.compat.libc.LibC
	depends embox.compat.posix.LibPosix
	depends embox.framework.LibFramework
}
