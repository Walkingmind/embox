package embox.cmd

@Cmd(name = "service",
	help = "run a system service",
	man = '''
		NAME
			service - run a system service
		SYNOPSIS
			service SCRIPT COMMAND
			service --status-all
			service --help | -h
		DESCRIPTION
			Run a system service
		AUTHORS
			Ilia Vaprol
	''')
module service {
	source "service.c"

	depends embox.cmd.net.servd
	depends embox.util.Array
	depends embox.cmd.sh.shell
	depends embox.compat.libc.LibC
	depends embox.framework.LibFramework
}
