
package('embox.fs.driver')

module(
	'fat',

  	options = [
		Integer('inode_quantity', default=64),
 		Integer('fat_descriptor_quantity', default=4),
	],

 	sources = [
		'fat.c',
 		'fatfs_subr.c',
	],

 	depends = [
		'embox.fs.node',
 		'embox.fs.driver.repo',
 		'embox.fs.driver.devfs',
 		'embox.mem.page_api',
 		'embox.mem.pool',
	],

) 
