package embox.driver.net

module loopback {
	source "loopback.c"
	make flags "-I$(SRC_DIR)/compat/posix/include"
	make flags "-I$(SRC_DIR)/compat/linux/include"
}

module ne2k_pci {
	source "ne2k_pci.c"
	depends embox.kernel.irq
	depends embox.driver.pci
	make flags "-I$(SRC_DIR)/compat/posix/include"
	make flags "-I$(SRC_DIR)/compat/linux/include"
}

module xemaclite {
	source "xilinx_emaclite.c"
	depends embox.kernel.irq
}

module r6040 {
	source "r6040.c"
	depends embox.kernel.irq
}
