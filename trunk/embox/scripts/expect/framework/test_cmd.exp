proc test_assert_regexp_equal {cmd success_output} {
	send $cmd
	set cmd_name [lindex [split $cmd " "] 0]
	expect "$cmd"
	expect {
	         timeout { puts "$cmd_name timeout\n"; exit 1 }
	        -regexp "$cmd_name:.*" { puts "$expect_out(buffer)\n"; exit 1  }
	        "$success_output" { }
	}
	return 0
}
