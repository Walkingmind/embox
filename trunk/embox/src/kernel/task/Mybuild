package embox.kernel

abstract module task_api { }

module util {
	source "index_desc.c", "file.c"
	depends embox.fs.rootfs
}

module multi_task extends task_api {
	source "multi_task.c", "def_task.c"
	depends embox.driver.virtual.diag_device
	depends util
	depends embox.kernel.thread.sched
}

module single_task extends task_api {
	source "single_task.c", "def_task.c"
	depends embox.driver.virtual.diag_device
	depends util
}
