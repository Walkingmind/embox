#ifndef TYPEINFO_
#define TYPEINFO_

#if __EXCEPTIONS==0
#include <standalone_typeinfo>
#elif __EXCEPTIONS==1
#include_next <typeinfo>
#else
#error __EXCEPTIONS macro should be defined to either 1 or 0
#endif

#endif /* TYPEINFO_ */
