package embox.net

module udp {
	source "udp.c"

	depends ipv4 /* for ip_output.c */
	depends skbuff
	depends icmpv4
	depends core /* for sock.c */
	depends embox.lib.LibC
	depends embox.framework.LibFrameworkNet
}

module tcp {
	source "tcp.c"

	depends tcp_sock
	depends skbuff
	depends core
	depends ipv4 /* for ip_output.c */
	depends af_inet /* for af_inet.c */
	depends embox.compat.posix.net.inet_addr
	depends embox.kernel.time.kernel_time
	depends embox.kernel.time.clock_source
	depends embox.kernel.critical
	depends embox.lib.LibC
	depends embox.kernel.timer.sys_timer
	depends embox.framework.LibFrameworkNet
}
