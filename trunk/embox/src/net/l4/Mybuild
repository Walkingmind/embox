package embox.net

module tcp {
	source "tcp.c"
	option boolean crc_drop = true

	depends embox.fs.idesc_event
	depends embox.net.skbuff
	depends embox.net.sock
	depends embox.net.lib.tcp
	depends embox.compat.posix.net.inet_addr
	depends embox.kernel.time.kernel_time
	depends embox.compat.libc.LibCAssert
	depends embox.compat.libc.LibCString
	depends embox.kernel.timer.sys_timer
	depends embox.framework.net.proto
}

module udp {
	source "udp.c"

	depends embox.net.lib.udp
	depends embox.net.skbuff
	depends embox.net.icmpv4
	depends embox.net.sock
	depends embox.compat.libc.LibCAssert
	depends embox.framework.net.proto
}
